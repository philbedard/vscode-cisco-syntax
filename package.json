{
    "name": "iosxr",
    "displayName": "Cisco IOS-XR Syntax",
    "description": "Cisco IOS-XR Syntax Highlighter",
    "version": "1.0.0",
    "publisher": "philbedard",
    "icon": "images/cisco.jpg",
    "license": "MIT",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phbedard/vscode-cisco-xr-syntax.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cisco-iosxr",
                "aliases": [
                    "iosxr", 
                    "ios-xr" 
                ],
                "extensions": [
                    ".xr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cisco-iosxr",
                "scopeName": "text.iosxr",
                "path": "./syntaxes/iosxr.tmGrammar.json"  
            }
        ]
    }
}
