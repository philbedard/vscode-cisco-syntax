{
    "name": "iosxr-test",
    "displayName": "Cisco IOS-XR Syntax Testing",
    "description": "Cisco IOS-XR Syntax Testing",
    "version": "2.0.0",
    "publisher": "Phil Bedard",
    "icon": "images/cisco.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/philbedard/vscode-cisco-xr-syntax.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cisco-iosxr-test",
                "extensions": [
                    ".xrt"
                ],
                "configuration": "./language-configuration-test.json"
            }
        ],
        "snippets": [
            {
                "language": "cisco-iosxr-test",
                "path": "./snippets/snippets.iosxr.json"
            }
        ],
        "grammars": [
            {
                "language": "cisco-iosxr-test",
                "scopeName": "text.iosxr-test",
                "path": "./syntaxes/iosxr.tmGrammar.test.json"  
            }
        ]
    }
}
