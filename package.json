{
    "name": "iosxr",
    "displayName": "Cisco IOS-XR Syntax",
    "description": "Cisco IOS-XR Syntax Highlighter",
    "version": "1.0.1",
    "publisher": "philxor",
    "icon": "images/cisco.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/philbedard/vscode-cisco-xr-syntax.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cisco-iosxr",
                "aliases": [
                    "iosxr",
                    "ios-xr"
                ],
                "extensions": [
                    ".xr",
                    ".iosxr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "snippets": [
            {
                "language": "cisco-iosxr",
                "path": "./snippets/snippets.iosxr.json"
            }
        ],
        "grammars": [
            {
                "language": "cisco-iosxr",
                "scopeName": "text.iosxr",
                "path": "./syntaxes/iosxr.tmGrammar.repo.json"
            }
        ]
    }
}
